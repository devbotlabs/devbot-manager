.dashboard-view
    lost-utility clearfix
    margin-top 20px
.dashboard-view > div
    border-radius 0 !important
    lost-column 1/3
    margin-bottom 30px

.dashboard-view > div > div > div > button
    &:nth-of-type(1)
        float left
    &:nth-of-type(2)
        float right
.dashboard-view > div > div > div .asd
    clear right !important
