{
  "name": "devbot-manager",
  "version": "0.0.1",
  "description": "admin panel for devbot",
  "author": "Devbot Labs (Enrico Foschi, Adrian JÃ¤ger, Alessio Scarapazzi)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devbotlabs/devbot-manager.git"
  },
  "scripts": {
    "prod": "npm run build && NODE_ENV=production node --harmony server.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js --separate-stylesheet -p --progress --profile --colors",
    "hot-dev-server": "webpack-dev-server --content-base public -ds --config webpack.config.js --hot --progress --colors --inline",
    "dev-server": "npm run hot-dev-server",
    "buildMeteor": "cd meteor && meteor build '../meteorBuild'",
    "unpackMeteor": "cd meteorBuild && tar -xzf 'meteor.tar.gz'",
    "moveMeteor": "mv meteorBuild/bundle/programs/web.browser/**.js src/meteor.bundle.js",
    "ripMeteor": "node ripMeteorScripts.js",
    "buildreact": "NODE_ENV=production webpack --progress --config webpack.build-react.config.js",
    "nginx-dev": "nginx/start-dev.sh",
    "nginx-build": "nginx/build.sh",
    "startmeteor": "scripts/start-meteor.sh"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.35",
    "babel-loader": "^5.3.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-tape-runner": "^1.2.0",
    "babelify": "^6.1.3",
    "blue-tape": "^0.1.10",
    "browserify": "^10.2.6",
    "css-loader": "^0.16.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^3.3.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "html-webpack-plugin": "^1.6.1",
    "lodash": "^4.5.1",
    "lost": "^6.6.2",
    "needle": "^0.11.0",
    "postcss-loader": "^0.6.0",
    "precss": "^1.2.3",
    "promise": "^7.0.4",
    "react-addons-css-transition-group": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "source-map-support": "0.3.2",
    "style-loader": "^0.12.3",
    "stylus": "latest",
    "stylus-loader": "^1.3.1",
    "transfer-webpack-plugin": "^0.1.4",
    "uglifyjs": "~2.4.10",
    "watchify": "^3.2.3",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1",
    "yargs": "^4.2.0"
  },
  "dependencies": {
    "formsy-material-ui": "0.3.2",
    "formsy-react": "^0.17.0",
    "hammerjs": "^2.0.4",
    "history": "^1.12.5",
    "material-ui": "0.13.3",
    "moment": "2.10.6",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-infinite": "^0.7.1",
    "react-infinite-scroll": "^0.1.5",
    "react-list": "^0.7.8",
    "react-router": "1.0.0-rc3",
    "react-static-container": "^1.0.0",
    "react-tap-event-plugin": "^0.2.1"
  }
}
